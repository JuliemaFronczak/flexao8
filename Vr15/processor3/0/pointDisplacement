FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    frontback
    {
        type            empty;
    }
    topbottom
    {
        type            uniformFixedValue;
        value           uniform (0 0 0);
        uniformValue    constant (0 0 0);
    }
    outlet
    {
        type            uniformFixedValue;
        value           uniform (0 0 0);
        uniformValue    constant (0 0 0);
    }
    inlet
    {
        type            uniformFixedValue;
        value           nonuniform List<vector> 0();
        uniformValue    constant (0 0 0);
    }
    walls
    {
        type            oscillatingDisplacement;
        amplitude       (0 0.08 0);
        omega           0.05236;
        value           uniform (0 0 0);
    }
    procBoundary3to1
    {
        type            processor;
    }
    procBoundary3to2
    {
        type            processor;
    }
}


